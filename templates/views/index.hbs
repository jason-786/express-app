<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
    <link rel="stylesheet" href="./css/style.css" />
    <title>Dyname-HBS</title>
  </head>
  <body>
    <div class="h-100">
    {{>header}}
    <h1 class="xyz">Dynamic Content</h1>
    <ul>
      {{#each arr}}
      <!-- {{title}} -->
        <li>{{this.name}}: {{this.reg}}</li>
      {{/each}}
    </ul>
    <form id="form">
      <h3 class="text-center mb-3">Form</h3>
      <p class="mb-0"><strong>Category</strong></p>
      <p>Episodes, Characters, Quotes</p>
      <div class="form-group">
      <label for="">Name</label>
      <input class="form-control mb-4" type="text" id="name">
    </div>
      <div class="form-group">
      <label for="">Number</label>
      <input class="form-control mb-4" type="text" id="number">
    </div>
      <input class="btn btn-danger" type="submit" value="Submit">
      <div id="data-message" class="fontsize-2 mt-4">
        No Data Found
      </div>
      <div id="error-message" class="fontsize-2 mb-4">
      </div>
    </form>
 {{>footer}}
 